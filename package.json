{
  "name": "nextjs-ts-tailwind-template",
  "version": "0.1.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "next:lint": "next lint",
    "lint": "pnpm run next:lint && eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "pnpm run next:lint && eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,scss}'",
    "prepare": "husky install",
    "check-types": "tsc",
    "validate": "pnpm run prettier && pnpm run lint && pnpm run check-types && pnpm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@linear/sdk": "^24.0.0",
    "@logtail/next": "^0.1.5",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@tanstack/react-query": "^5.50.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "encoding": "^0.1.13",
    "jotai": "^2.9.0",
    "jotai-devtools": "^0.10.0",
    "lucide-react": "^0.404.0",
    "next": "^14.1.0",
    "next-themes": "^0.3.0",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.1",
    "sass": "^1.70.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tsconfig/next": "^2.0.1",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.5",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm run lint"
    ],
    "*.{js,jsx,ts,tsx,scss}": [
      "pnpm run prettier"
    ]
  }
}
